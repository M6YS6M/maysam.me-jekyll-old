footer
 .container.beautiful-jekyll-footer
   .row
     .col-lg-8.col-lg-offset-2.col-md-10.col-md-offset-1
       ul.list-inline.text-center.footer-links
         | {% if site.author.facebook and site.footer-links-active.facebook %}
         li
           a(href='https://www.facebook.com/{{ site.author.facebook }}', title='Facebook')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-facebook.fa-stack-1x.fa-inverse
         |           {% endif %}
         |           {% if site.author.github and site.footer-links-active.github %}
         li
           a(href='https://github.com/{{ site.author.github }}', title='GitHub')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-github.fa-stack-1x.fa-inverse
         |           {% endif %}
         | 		  {% if site.author.twitter and site.footer-links-active.twitter %}
         li
           a(href='https://twitter.com/{{ site.author.twitter }}', title='Twitter')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-twitter.fa-stack-1x.fa-inverse
         |           {% endif %}
         |      {% if site.author.instagram and site.footer-links-active.instagram %}
         li
           a(href='https://www.instagram.com/{{ site.author.instagram }}', title='Instagram')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-instagram.fa-stack-1x.fa-inverse
         |           {% endif %}
         | 		  {% if site.author.email and site.footer-links-active.email %}
         li
           a(href='mailto:{{ site.author.email }}', title='Email me')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-envelope.fa-stack-1x.fa-inverse
         |           {% endif %}
         | 		  {% if site.author.linkedin and site.footer-links-active.linkedin %}
         li
           a(href='https://linkedin.com/in/{{ site.author.linkedin }}', title='LinkedIn')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-linkedin.fa-stack-1x.fa-inverse
         |           {% endif %}
         | 		  {% if site.author.stackoverflow and site.footer-links-active.stackoverflow %}
         li
           a(href='https://stackoverflow.com/story/{{ site.author.stackoverflow }}', title='StackOverflow')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-stack-overflow.fa-stack-1x.fa-inverse
         |           {% endif %}
         | 		  {% if site.author.mathexchange and site.footer-links-active.mathexchange %}
         li
           a(href='https://math.stackexchange.com/users/{{ site.author.mathexchange }}', title='MathExchange')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-stack-exchange.fa-stack-1x.fa-inverse
         |           {% endif %}
         | 		  {% if site.footer-links-active.rss %}
         li
           a(href="{{ '/feed.xml' | prepend: site.baseurl }}", title='RSS')
             span.fa-stack.fa-lg
               i.fa.fa-circle.fa-stack-2x
               i.fa.fa-rss.fa-stack-1x.fa-inverse
         |           {% endif %}
       p.copyright.text-muted
         | {{ site.author.name }}
         | • 
         | {{ site.time | date: '%Y' }}
         | {% if site.url-pretty %}
         | • 
         a(href='{{ site.url }}') {{ site.url-pretty }}
         | 		  {% endif %}
       // Please don't remove this, keep my open source work credited :)
       p.theme-by.text-muted
         | Theme by
         a(href='http://deanattali.com/beautiful-jekyll/') beautiful-jekyll


