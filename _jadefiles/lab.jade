page.lab
  .flex.flex--center
    
      
    
      .lab-wrap
        {% assign items = site.lab | sort: 'date' | reverse  %}
        {% for item in items  %}
        a(href="{{ site.baseurl }}{{item.url}}" style="background-image: url({{ site.baseurl }}{{item.image_path}})").lab-thumb
          .lab-meta
            .name {{item.title}}
            .date {{item.date | date: " %b , %Y"}}
          {% endfor %}
          
